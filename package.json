{
	"name": "opus-transcriber-proxy",
	"version": "0.0.0",
	"private": true,
	"license": "Apache-2.0",
	"scripts": {
		"format": "prettier --write '**/*.{js,ts,css,json,md}'",
		"configure": "make configure -j$(node -e 'console.log(require(\"os\").cpus().length)')",
		"build:wasm": "make -j$(node -e 'console.log(require(\"os\").cpus().length)')",
		"build:ts": "tsc",
		"build": "npm run build:wasm && npm run build:ts",
		"dev": "npm run build:wasm && tsx watch src/server.ts",
		"start": "node dist/src/server.js",
		"typecheck": "tsc --noEmit",
		"test": "vitest"
	},
	"dependencies": {
		"dotenv": "^16.4.5",
		"ws": "^8.18.0"
	},
	"devDependencies": {
		"@types/node": "^24.7.2",
		"@types/ws": "^8.5.13",
		"prettier": "^3.6.2",
		"tsx": "^4.19.2",
		"typescript": "^5.9.3",
		"vitest": "~3.2.0"
	}
}
